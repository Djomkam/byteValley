<!DOCTYPE html>
<html lang="en" 
	xmlns:th="http://www.thymeleaf.org" 
	xmlns:sec="http://www.thymeleaf.org/thymeleaf-extras-springsecurity3"
	xmlns:social="http://spring.io/springsocial">
  <head th:replace="fragments/head :: facebook-head">
    
  </head>
  <body class="skin-purple sidebar-mini">
    <div class="wrapper">

      <!-- Main Header -->
      <header th:replace="fragments/header :: header">

      </header>
      <!-- Left side column. contains the logo and sidebar -->
      <aside th:replace="fragments/sidebar :: facebook-sidebar" >

      </aside>

      <!-- Content Wrapper. Contains page content -->
      <div class="content-wrapper" style="height:960px;">
        <!-- Content Header (Page header) -->
        <section class="content-header">
          <h1>
            Page Header
            <small>Optional description</small>
          </h1>
          <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Level</a></li>
            <li class="active">Here</li>
          </ol>
        </section>

        <!-- Main content -->
        <section class="content">
			<section class="col-lg-12 connectedSortable">
				<div class="panel panel-default" style="height:905px;overflow-y:auto">
					<div class="panel-heading">
						<h4 style="color: black;font-family: calibri">Your Facebook Feeds</h4>
					</div>
					<div class="panel-body">
						<div id="page-wrap">
							<h3>Your LinkedIn Profile</h3>
			
							<p>Hello, <span th:text="${profile.firstName}">first name</span>!</p>
							<img th:src="${profile.profilePictureUrl}"/>
							<dl>
								<dt>LinkedIn ID:</dt>
								<dd><a th:href="${profile.publicProfileUrl}" target="_blank" th:text="${profile.id}">profile id</a></dd>
								<dt>Email Address:</dt>
								<dd th:text="${profile.emailAddress}"></dd>
								<dt>Headline:</dt>
								<dd th:text="${profile.headline}"></dd>
								<dt>Industry:</dt>
								<dd th:text="${profile.industry}"></dd>
								<dt>Summary:</dt>
								<dd th:text="${profile.summary}"></dd>
							</dl>
						</div>
					</div>
				</div>
			</section>
        </section><!-- /.content -->
      </div><!-- /.content-wrapper -->
	  <footer th:replace="fragments/footer :: footer">
	  
	  </footer>
    </div><!-- ./wrapper -->

    <!-- REQUIRED JS SCRIPTS -->

    <script src="http://cdn.jsdelivr.net/webjars/bootstrap/3.3.2/bootstrap.min.js"></script>
    <script src="http://cdn.jsdelivr.net/webjars/jquery/3.0.0-alpha1/jquery.min.js"></script>
	<script src="http://cdn.jsdelivr.net/webjars/datatables/1.10.7/jquery.dataTables.min.js"></script>
	<script src="http://cdn.jsdelivr.net/webjars/jQuery-slimScroll/1.3.3/jquery.slimscroll.min.js"></script>
    <script src="../static/js/app.min.js" th:src="@{/js/app.min.js}" type="text/javascript"></script>  
	<script type="text/javascript">
		$(function(){
			alert("jznfdjvv");
			$("#page-wrap").slimScroll({
				height: '785px',
				width: '100%',
				scrollTo: '50px',
				size: '5px'
			});
			$("#myTable").DataTable({
				"info": false,
				"searching": false,
				"ordering": false,
				"pageLength": 10
			});
		});
	</script>
  </body>
</html>
