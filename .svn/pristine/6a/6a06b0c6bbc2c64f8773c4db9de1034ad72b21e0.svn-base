<!DOCTYPE html>
<html lang='en' 
	xmlns:th='http://www.thymeleaf.org' 
	xmlns:sec='http://www.thymeleaf.org/thymeleaf-extras-springsecurity3'
	xmlns:social='http://spring.io/springsocial'>
  <head th:replace='fragments/head :: th-head'>
    
  </head>
  <body class='skin-black sidebar-mini'>
    <div class='wrapper'>

      <!-- Main Header -->
      <header th:replace='fragments/header :: header'>

      </header>
      <!-- Left side column. contains the logo and sidebar -->
      <aside th:replace='fragments/sidebar :: th-sidebar'>

      </aside>

      <!-- Content Wrapper. Contains page content -->
      <div class='content-wrapper'>
        <!-- Content Header (Page header) -->
        <section class='content-header'>
          <h1>
            Page Header
            <small>Optional description</small>
          </h1>
          <ol class='breadcrumb'>
            <li><a href='#'><i class='fa fa-dashboard'></i> Level</a></li>
            <li class='active'>Here</li>
          </ol>
        </section>

        <!-- Main content -->
        <section class='content'>
			<section class='col-lg-12 connectedSortable'>
				<div class='panel panel-default'>
					<div class='panel-heading'>
						<h4 style='color: black;font-family: calibri'>Your Facebook Feeds</h4>
					</div>
					<div class='panel-body'>
						<div id='page-wrap'>
							<h3>Your Facebook Friends</h3>		
							<ul class='friends'>
								<li th:each='friend : ${friends}'><img th:src="'http://graph.facebook.com/' + ${friend.id} + '/picture'" align='middle'/><span th:text="${friend.name}">name</span></li>
							</ul>
						</div>
					</div>
				</div>
			</section>
        </section><!-- /.content -->
      </div><!-- /.content-wrapper -->
	  <footer th:replace='fragments/footer :: .th-footer'>
	  
	  </footer>
    </div><!-- ./wrapper -->

    <!-- REQUIRED JS SCRIPTS -->

    <div th:replace='fragments/appJS :: .th-appJS'>
    	
    </div>
  </body>
</html>
