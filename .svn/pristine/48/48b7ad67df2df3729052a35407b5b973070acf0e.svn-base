<!DOCTYPE html>
<html lang="en" 
	xmlns:th="http://www.thymeleaf.org" 
	xmlns:sec="http://www.thymeleaf.org/thymeleaf-extras-springsecurity3"
	xmlns:social="http://spring.io/springsocial">
  <head th:replace="/admin/fragments/head :: admin-head">
    
  </head>
  <body class="skin-purple sidebar-mini">
    <div class="wrapper">

      <!-- Main Header -->
      <header th:replace="/fragments/header :: header">

      </header>
      <!-- Left side column. contains the logo and sidebar -->
      <aside th:replace="/admin/fragments/sidebar :: admin-sidebar">

      </aside>

      <!-- Content Wrapper. Contains page content -->
      <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
          <h1>
            Page Header
            <small>Optional description</small>
          </h1>
          <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Level</a></li>
            <li class="active">Here</li>
          </ol>
        </section>

        <!-- Main content -->
        <section class="content">
			<section class="col-lg-6 connectedSortable">
				<div class="box box-primary">
					<div class="box-header">
						<i class="ion ion-clipboard"></i>
						<h3 class="box-title">Users</h3>
						<div class="box-tools">
							Users go here
						</div>
					</div>
					<div class="box-body">
						<ul>
							<li th:each="user:${users}" th:text="${user.username}">Username</li>
						</ul>
					</div>
					<div class="box-footer clearfix no-border">
					
					</div>
				</div>
				<div class="box box-primary">
					<div class="box-header">
						<i class="ion ion-clipboard"></i>
						<h3 class="box-title">Roles</h3>
						<div class="box-tools">
							Roles go here
						</div>
					</div>
					<div class="box-body">
						<ul>
							<li th:each="role:${roles}" th:text="${role.roleName}">Role Name</li>
						</ul>
					</div>
					<div class="box-footer clearfix no-border">
					
					</div>
				</div>
			</section>
			<section class="col-lg-6 connectedSortable">
				<div class="box box-primary">
					<div class="box-header">
						<i class="ion ion-clipboard"></i>
						<h3 class="box-title">Accounts</h3>
						<div class="box-tools">
							Accounts go here
						</div>
					</div>
					<div class="box-body">
						<ul>
							<li th:each="account:${accounts}" th:text="${account.accountName}">Account Name</li>
						</ul>
					</div>
					<div class="box-footer clearfix no-border">
					
					</div>
				</div>
			</section>
        </section><!-- /.content -->
      </div><!-- /.content-wrapper -->
	  <footer th:replace="/fragments/footer :: footer">
	  
	  </footer>
      <div class="control-sidebar-bg"></div>
    </div><!-- ./wrapper -->

    <!-- REQUIRED JS SCRIPTS -->

    <div th:replace="/admin/fragments/head :: #admin-jscode">
    
    </div>
  </body>
</html>
